<plugins xmlns="urn:org.androidaudioplugin.core" xmlns:pp="urn:org.androidaudioplugin.parameter-properties">
  <plugin name="Flat Filter" category="Effect" author="atsushieno" manufacturer="atsushieno" unique-id="urn:org.androidaudioplugin/samples/aapbarebonepluginsample/FlatFilter" library="libaapbarebonepluginsample.so">
    <parameters>
      <parameter direction="input" content="control" name="Output Volume L" pp:default="0.5" pp:minimum="0" pp:maximum="1" />
      <parameter direction="input" content="control" name="Output Volume R" pp:default="0.5" pp:minimum="0" pp:maximum="1" />
      <parameter direction="input" content="control" name="Delay L" pp:default="0" pp:minimum="0" pp:maximum="2048" pp:type="integer" />
      <parameter direction="input" content="control" name="Delay R" pp:default="256" pp:minimum="0" pp:maximum="2048" pp:type="integer" />
      <parameter direction="input" content="control" name="Dummy Parameter 4" pp:default="50" pp:minimum="0" pp:maximum="100" pp:type="integer" />
      <parameter direction="input" content="control" name="Dummy Parameter 5" pp:default="50" pp:minimum="0" pp:maximum="100" pp:type="integer" />
      <parameter direction="input" content="control" name="Dummy Parameter 6" pp:default="50" pp:minimum="0" pp:maximum="100" pp:type="integer" />
      <parameter direction="input" content="control" name="Dummy Parameter 7" pp:default="50" pp:minimum="0" pp:maximum="100" pp:type="integer" />
    </parameters>
    <ports>
      <port direction="input" content="audio" name="Left In" />
      <port direction="input" content="audio" name="Right In" />
      <port direction="output" content="audio" name="Left Out" />
      <port direction="output" content="audio" name="Right Out" />
      <port direction="input" content="midi2" name="MIDI In" />
      <port direction="output" content="midi2" name="MIDI Out" />
    </ports>
  </plugin>
</plugins>
